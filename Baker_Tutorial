Step 10: Binning Concoct
#PBS -S /bin/bash
#PBS -q joye_q
#PBS -N testConcoct_PE1031_Sed_54
#PBS -l nodes=1:ppn=20
#PBS -l walltime=80:00:00
#PBS -l mem=200g
#PBS -j oe

#PBS -m ae


